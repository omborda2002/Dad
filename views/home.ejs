    <script>
        /* When the user clicks on the button, 
        toggle between hiding and showing the dropdown content */
        function myFunction() {
            if (screen.width <= 460) {
                let count = 0;
                let x = document.querySelector('.ddown')
                if (count == 1) {
                    x.style.display = "block";
                    count++;
                }
                if (x.style.display === "none") {
                    x.style.display = "block";
                } else {
                    x.style.display = "none";
                }
            }
        }
    </script>

